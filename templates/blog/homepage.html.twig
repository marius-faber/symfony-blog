{% extends 'base.html.twig' %}

{% block body %}
    {% for post in posts %}
        <div class="card mt-5">
            <div class="card-body">
                <h5 class="card-title">{{ post.title }}</h5>
                <p class="card-text">{{ post.content }}</p>
            </div>
        </div>
    {% endfor %}
{% endblock %}